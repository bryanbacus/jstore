<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.shenming.sms.module.hibernateOrm.SmTbFlowerIntor" lazy="false" table="sm_tb_flower_intor">
	<id
        name="itemSeq"
        type="integer"
        column="item_seq"
        length="11"
    >
		<generator class="identity" />
	</id>
	<property
        name="description"
        type="string"
        column="description"
        length="500"
    />
	<property
        name="isShow"
        type="string"
        column="is_show"
        length="10"
    />
	<property
        name="itemCharacter"
        type="string"
        column="item_character"
        length="50"
    />
	<property
        name="itemPics"
        type="string"
        column="item_pics"
        length="200"
    />
	<property
        name="title"
        type="string"
        column="title"
        length="50"
    />
	<property
        name="usages"
        type="string"
        column="usages"
        length="50"
    />

	<!-- Associations -->

</class>
</hibernate-mapping>
