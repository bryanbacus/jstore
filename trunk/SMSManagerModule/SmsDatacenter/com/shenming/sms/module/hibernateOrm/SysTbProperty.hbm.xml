<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.shenming.sms.module.hibernateOrm.SysTbProperty" lazy="false" table="sys_tb_properties">
	<id
        name="idx"
        type="integer"
        column="idx"
        length="11"
    >
		<generator class="identity" />
	</id>
	<property
        name="description"
        type="string"
        column="description"
        length="200"
    />
	<property
        name="keyGroup"
        type="string"
        column="key_group"
        length="20"
    />
	<property
        name="keyName"
        type="string"
        column="key_name"
        not-null="true"
        length="50"
    />
	<property
        name="keyValue"
        type="string"
        column="key_value"
        length="200"
    />
	<property
        name="modifyDate"
        type="date"
        column="modify_date"
        length="10"
    />

	<!-- Associations -->

</class>
</hibernate-mapping>
