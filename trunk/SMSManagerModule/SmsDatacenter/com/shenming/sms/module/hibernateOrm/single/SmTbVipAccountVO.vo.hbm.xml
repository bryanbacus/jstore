<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class 
    name="com.shenming.sms.module.hibernateOrm.single.SmTbVipAccountVO" 
    table="sm_tb_vip_account"
    lazy="false"
>
    <id
        name="userId"
        type="integer"
        column="user_id"
        length="11"
    >
        <generator class="assigned" />
    </id>
    <property
        name="address"
        type="string"
        column="address"
        length="100"
    />
    <property
        name="birthday"
        type="timestamp"
        column="birthday"
        length="19"
    />
    <property
        name="blood"
        type="string"
        column="blood"
        length="10"
    />
    <property
        name="buySum"
        type="long"
        column="buy_sum"
        not-null="true"
        length="22"
    />
    <property
        name="crDate"
        type="timestamp"
        column="cr_date"
        not-null="true"
        length="19"
    />
    <property
        name="errorCnt"
        type="integer"
        column="error_cnt"
        length="2"
    />
    <property
        name="fovarite"
        type="string"
        column="fovarite"
        length="200"
    />
    <property
        name="isAccountActive"
        type="string"
        column="is_account_active"
        not-null="true"
        length="1"
    />
    <property
        name="isValidVip"
        type="string"
        column="is_valid_vip"
        not-null="true"
        length="1"
    />
    <property
        name="job"
        type="string"
        column="job"
        length="100"
    />
    <property
        name="mailAddr"
        type="string"
        column="mail_addr"
        not-null="true"
        length="100"
    />
    <property
        name="marrage"
        type="string"
        column="marrage"
        length="10"
    />
    <property
        name="mobile"
        type="string"
        column="mobile"
        length="20"
    />
    <property
        name="owner"
        type="integer"
        column="owner"
        length="11"
    />
    <property
        name="password"
        type="string"
        column="password"
        not-null="true"
        length="200"
    />
    <property
        name="phone"
        type="string"
        column="phone"
        length="20"
    />
    <property
        name="sex"
        type="string"
        column="sex"
        length="10"
    />
    <property
        name="sid"
        type="string"
        column="sid"
        length="10"
    />
    <property
        name="store"
        type="string"
        column="store"
        length="50"
    />
    <property
        name="storeEmployeeName"
        type="string"
        column="store_employee_name"
        length="50"
    />
    <property
        name="username"
        type="string"
        column="username"
        not-null="true"
        unique="true"
        length="50"
    />
    <property
        name="vipChtName"
        type="string"
        column="vip_cht_name"
        not-null="true"
        length="50"
    />
    <property
        name="vipEngName"
        type="string"
        column="vip_eng_name"
        not-null="true"
        length="50"
    />
    <property
        name="vipLevel"
        type="string"
        column="vip_level"
        length="5"
    />

    <!-- Associations -->

</class>
</hibernate-mapping>
